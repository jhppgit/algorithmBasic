
public class Test {
	// 소수 출력 테스트(소수배열)
	public static void main(String[] args) {
		int count = 0;
		int ptr = 0; // 현재 소수 배열 길이
		int prime[] = new int[500]; // 소수 배열
		
		prime[ptr++] = 2; // 2부터 시작.
		for(int n = 3; n <= 1000; n += 2) { // 1000까지, 홀수만
			int i; // n으로 나눠볼 prime의 인덱스
			for(i = 1; i < ptr; i++) {
				count++;
				if (n % prime[i] == 0)
					break; // 나누어 떨어지면 그만.
			}
			if(i == ptr)
				prime[ptr++] = n; // 반복문 통과하면 소수임. 배열에 추가
		}
		
		for(int i = 0; i < ptr; i++) {
			System.out.println(prime[i]);
		}
		System.out.println("count : " + count);
	}
}
