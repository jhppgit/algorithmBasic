package algorithm.ch05;

// 유클리드 호제법으로 최대공약수를 구함
public class EuclidGCD {
	
	static int gcd(int x, int y) {
		if (y == 0)
			return x;
		else
			return gcd(y, x % y);
	}
	
	public static void main(String[] args) {
		Scanner stdIn = new Scanner(System.in);
		
		System.out.println("두 수의 최대공약수를 구합니다.");
	}
}
