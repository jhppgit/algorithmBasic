
public class Test {
	public static void main(String[] args) {
		// 제곱근까지만 검사.
		int count = 0;
		int ptr = 0;
		int prime[] = new int[500];
		
		prime[ptr++] = 2;
		prime[ptr++] = 3;
		for(int n = 5; n <= 1000; n += 2) {
			int i;
			boolean flag = false;
			for(i = 1; prime[i] * prime[i] <= n; i++) {
				count += 2;
				
				if(n % prime[i] == 0)
					flag = true;
					break;
			}
			if(!flag) {
				prime[ptr++] = n;
				count++;
			}
		}
		
		System.out.println(count);
	}
}
